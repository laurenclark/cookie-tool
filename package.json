{
    "name": "rawcookie",
    "version": "1.0.0",
    "author": "Lauren Clark @ Rawnet (lclark@rawnet.com)",
    "description": "PECR Cookie plugin for websites",
    "main": "index.js",
    "scripts": {
        "dev": "npm run clean && parcel public/index.html --out-dir development -p 3000",
        "build": "parcel build --no-source-maps public/index.html --out-dir dist --public-url ./",
        "clean": "rimraf ./development && rimraf -rf ./.cache && rimraf ./dist"
    },
    "browserslist": [
        "IE 11",
        "last 2 versions and > .2%",
        "not dead"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.10.3",
        "@babel/core": "^7.0.0-0",
        "@babel/plugin-transform-runtime": "^7.10.3",
        "@babel/preset-env": "^7.10.3",
        "@babel/runtime-corejs2": "^7.10.3",
        "autoprefixer": "^9.8.2",
        "babel-preset-env": "^1.7.0",
        "details-polyfill": "^1.1.0",
        "parcel-bundler": "^1.6.1",
        "rimraf": "^3.0.2",
        "sass": "^1.26.9"
    },
    "dependencies": {},
    "postcss": {
        "plugins": {
            "autoprefixer": {
                "overrideBrowserslist": [
                    "> 1%",
                    "last 2 versions",
                    "ie 11"
                ]
            }
        }
    }
}
